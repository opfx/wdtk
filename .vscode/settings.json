{
  "jira-plugin.workingProject": "",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".yarn": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/target": true,
    "**/jest.config.js": false,
    "**/tsconfig.json": true
  },
  "cSpell.words": [
    "Codelyzer",
    "dasherize",
    "devkit",
    "nestjs",
    "pacote",
    "wdtk"
  ],
  "cSpell.ignorePaths": ["**/package.json", "**/yarn.lock", "**/node_modules/**", "**/vscode-extension/**", "**/.git/objects/**", ".vscode"]
}
