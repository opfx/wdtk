{
  "name": "@wdtk/workspace",
  "version": "0.1",
  "schematics": {
    "application": {
      "aliases": ["app"],
      "description": "Creates an application",
      "factory": "./schematics/application/index",
      "schema": "./schematics/application/schema.json"
    },
    "ng-new": {
      "description": "Creates a new workspace",
      "factory": "./schematics/ng-new/index",
      "schema": "./schematics/ng-new/schema.json"
    },
    "typescript": {
      "description": "Creates an empty workspace",
      "factory": "./schematics/typescript/index",
      "schema": "./schematics/typescript/schema.json",
      "hidden": true
    },
    "workspace": {
      "description": "Creates an empty workspace",
      "factory": "./schematics/workspace/index",
      "schema": "./schematics/workspace/schema.json",
      "hidden": true
    }
  }
}
