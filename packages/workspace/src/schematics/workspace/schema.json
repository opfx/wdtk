{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsWorkspaceWorkspace",
  "title": "Creates an empty workspace.",
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the workspace.",
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for the workspace?"
    }
  },
  "required": ["name"]
}
