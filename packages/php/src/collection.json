{
  "name": "@wdtk/php",
  "version": "0.1",
  "schematics": {
    "application": {
      "aliases": ["app"],
      "description": "Creates a PHP application.",
      "factory": "./schematics/application/index",
      "schema": "./schematics/application/schema.json"
    },
    "init": {
      "aliases": ["ng-add"],
      "description": "Adds PHP nature to the workspace.",
      "factory": "./schematics/init/index",
      "schema": "./schematics/init/schema.json",
      "hidden": true
    },
    "library": {
      "aliases": ["lib"],
      "description": "Creates a PHP library.",
      "factory": "./schematics/library/index",
      "schema": "./schematics/library/schema.json"
    },
    "project": {
      "description": "Configures a PHP project.",
      "factory": "./schematics/project/index",
      "schema": "./schematics/project/schema.json",
      "hidden": true
    }
  }
}
