{
  "$schema": "@angular-devkit/architect/src/builders-schema.json",
  "builders": {
    "build": {
      "implementation": "./builders/build/index",
      "schema": "./builders/build/schema.json",
      "description": "Build a PHP project."
    },
    "lint": {
      "implementation": "./builders/build/index",
      "schema": "./builders/build/schema.json",
      "description": "Lints a PHP project."
    },
    "serve": {
      "implementation": "./builders/serve/index",
      "schema": "./builders/serve/schema.json",
      "description": "Serve a PHP application."
    },

    "test": {
      "implementation": "./builders/build/index",
      "schema": "./builders/build/schema.json",
      "description": "Runs PHP unit tests."
    },
    "e2e": {
      "implementation": "./builders/build/index",
      "schema": "./builders/build/schema.json",
      "description": "Runs PHP end to end (e2e) tests."
    }
  }
}
